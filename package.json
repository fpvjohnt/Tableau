{
  "name": "tableau-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for intelligent Tableau automation and control",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "start:network": "node dist/network-server.js",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "dev:network": "tsx src/network-server.ts",
    "watch": "tsx watch src/index.ts"
  },
  "keywords": [
    "mcp",
    "tableau",
    "automation",
    "ai",
    "claude"
  ],
  "author": "Claude AI Assistant",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
